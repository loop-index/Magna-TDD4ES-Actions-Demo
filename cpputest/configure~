   ;; #(
  tcc) :

  ax_cv_c_compiler_version=`tcc -v | $SED 's/^[ ]*tcc[ ]\+version[ ]\+\([0-9.]\+\).*/\1/g'`
   ;; #(
  sdcc) :

  if ac_fn_c_compute_int "$LINENO" "/* avoid parse error with comments */
    #if(defined(__SDCC_VERSION_MAJOR))
	__SDCC_VERSION_MAJOR
    #else
	SDCC/100
    #endif
    " "_ax_c_compiler_version_major"        ""
