  ax_cv_c_compiler_version="$_ax_c_compiler_version_major.$_ax_c_compiler_version_minor.$_ax_c_compiler_version_patch"
   ;; #(
  sgi) :


  if ac_fn_c_compute_int "$LINENO" "
	     #if defined(_COMPILER_VERSION)
