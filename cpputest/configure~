  if ac_fn_c_compute_int "$LINENO" "(
	     #if defined(_COMPILER_VERSION)
	     _COMPILER_VERSION
	     #else
	     _SGI_COMPILER_VERSION
	     #endif
	    /100)%10" "_ax_c_compiler_version_major"        ""
