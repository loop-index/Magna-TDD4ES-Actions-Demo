  _ax_c_compiler_version_patch=0
fi

fi
  # for version 7
  if test "X$_ax_c_compiler_version_major" = "X13"
then :
  if ac_fn_c_compute_int "$LINENO" "_MSC_FULL_VER%1000" "_ax_c_compiler_version_patch"        ""
